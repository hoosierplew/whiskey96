<%= simple_form_for(@whiskey) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :type %>
    <%= f.input :tags %>
    <%= f.input :distillery %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :image %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
